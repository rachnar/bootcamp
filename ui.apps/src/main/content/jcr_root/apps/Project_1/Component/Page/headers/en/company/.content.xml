            <text_2
                jcr:created="{Date}2010-11-05T14:41:15.120Z"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2010-11-05T14:41:36.785Z"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/text"
                text="&lt;p>&lt;span class=&quot;large&quot;>Geometrixx was founded in 545 BC by Pythagoras of Samos. Since then we have expanded from our ancient Greek origins to become a worldwide geometry and topology provider. We now have offices in Boston, Los Angeles, Basel and Singapore.&lt;/span>&lt;/p>&#xa;"
                textIsRich="true"/>
            <image
                jcr:created="{Date}2010-11-11T16:20:39.301+01:00"
